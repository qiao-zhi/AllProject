package arithmeticTest;

import javax.xml.transform.Templates;

/**
 * 快速排序
 * 
 * @author: qlq
 * @date : 2018年4月2日下午10:33:22
 */
public class KuaisuSort {

	public static void main(String[] args) {
		
	}

	/**
	 * 排序主要算法	5 6 6 5 8 8
	 * 
	 * @param arr
	 */
	public void quickSort(int arr[],int start,int end) {
		if(start==end){	//如果开始=结束就结束排序
			return;
		}
		int pivot = arr[start];//选取第一个作为基准
		for(int i=start;i<end;i++){//遍历数组
			if( arr[i]<pivot ){
				temp = arr[i];
				
			}
		}
		
	}

}
