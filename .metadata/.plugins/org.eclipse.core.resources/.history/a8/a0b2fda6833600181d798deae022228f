 */
public class KuaisuSort {

	public static void main(String[] args) {
		
	}

	/**
	 * 排序主要算法	6 9 6 5 8 8
	 * 
	 * @param arr
	 */
	public void quickSort(int arr[],int start,int end) {
		if(arr.length==1){
			return;
		}
		int pivot = arr[0];//选取第一个作为基准
		for(int i=1;i<arr.length;i++){//遍历数组
			if( arr[i]<pivot ){
				
			}
		}
		
	}

}
