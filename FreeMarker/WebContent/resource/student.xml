<?xml version="1.0" encoding="UTF-8"?>
<excel id="student" code="student" name="学生信息导入">
    <colgroup>
        <col index="A" width='17em'></col>
        <col index="B" width='17em'></col>
        <col index="C" width='17em'></col>
        <col index="D" width='17em'></col>
        <col index="E" width='17em'></col>
        <col index="F" width='17em'></col>        
        <col index="G" width='17em'></col>        
        <col index="H" width='17em'></col>        
    </colgroup>
    <title>
        <tr height="16px">
            <td rowspan="1" colspan="8" value="学生信息导入" />
        </tr>
    </title>
    <thead>
        <tr height="16px">
        	<th value="题干" />
            <th value="类型" />
            <th value="A选项" />
            <th value="B选项" />
            <th value="C选项" />
            <th value=" D选项" />            
            <th value="答案" />            
            <th value="解析" />            
        </tr>
    </thead>
    <tbody>
        <tr height="16px" firstrow="2" firstcol="0" repeat="100">
            <td type="string" isnullable="false" maxlength="30" /><!--题干 -->
            <td type="enum" format="单选,多选,判断" isnullable="true" /><!--类型 -->
            <td type="string" isnullable="false" maxlength="30" /><!--A选项 -->
            <td type="string" isnullable="false" maxlength="30" /><!--B选项 -->
            <td type="string" isnullable="false" maxlength="30" /><!--C选项 -->
            <td type="string" isnullable="false" maxlength="30" /><!--D选项 -->
            <td type="string" isnullable="false" maxlength="50" /><!--答案 -->
            <td type="string" isnullable="false" maxlength="50" /><!--解析 -->
        </tr>
    </tbody>
</excel>